{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
       
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js",
        "headers": {
        "Access-Control-Allow-Origin": "https://travler.vercel.app,http://localhost:5173",
        "Access-Control-Allow-Credentials": "true"
      }
      }
    ]
  }